import java.util.Scanner;
  public class A{
	  public static void main (String args[]){
		 Scanner n =new Scanner(System.in);
		 System.out.println("Given number is even  : ");
		 int a =n.nextInt();
		 if (a%2==0)
         System.out.println("even number");
		 else 

		 System.out.println("odd number");
		  
	  }
  }
		   
		
